  <data name="OnboardingWizard_Wave1_ControlDial_Description" xml:space="preserve">
    <value>Verwenden Sie das Rädchen, um wichtige Mikrofonfunktionen schnell zu steuern:</value>
  </data>
  <data name="OnboardingWizard_ExploreDevice_Description" xml:space="preserve">
